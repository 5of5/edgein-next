{
  "name": "edgein",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:production": "next dev",
    "build": "NODE_OPTIONS=\"--max_old_space_size=4096\" next build",
    "start": "next start",
    "test": "jest --watch",
    "lint": "next lint",
    "lint:fix": "npm run eslint-and-fix && npm run prettier-fix",
    "eslint": "eslint . --ext .ts --ext .tsx",
    "eslint-and-fix": "eslint . --ext .ts --ext .tsx --fix",
    "prettier-check": "prettier --check **",
    "prettier-fix": "prettier --write **",
    "prepare-husky": "husky install",
    "docker-compose": "docker compose -f ./infra/docker-compose.yml --env-file ./infra/.dockerenv",
    "docker:start": "npm run docker-compose -- up -d",
    "docker:stop": "npm run docker-compose -- down",
    "hasura": "cd infra/hasura && hasura --skip-update-check",
    "hasura:migrate": "npm run hasura migrate apply && npm run hasura metadata apply",
    "codegen": "graphql-codegen --config graphql/codegen.js",
    "db:seed": "cd scripts/db && sh load-seed-data.sh",
    "api:test": "jest --config=test-api/api-test.config.js --testPathPattern=test-api",
    "api:build": "cd server-api/ && rimraf build && npx tsc && cp .env build/server-api",
    "api:dev": "cd server-api/ && set NODE_ENV=development && nodemon -e ts --exec ts-node -r tsconfig-paths/register index.ts",
    "api:start": "cd server-api/ && pm2 start --interpreter none 'node -r ts-node/register/transpile-only -r tsconfig-paths/register build/server-api/index.js'",
    "playwright:test": "npx playwright test",
    "test:e2e": "npm run docker:start && npm run playwright:test"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
