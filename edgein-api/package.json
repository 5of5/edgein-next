{
  "name": "edgein-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "metadata:build": "npx ts-node src/generate-metadata.ts",
    "metadata:watch": "yarn run metadata:build --watch",
    "doc:console": "npx @compodoc/compodoc -r 8088 -p tsconfig.json -s -w",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codegen": "graphql-codegen --config libs/sdk/codegen.ts"
  },
  "dependencies": {
    "@graphql-yoga/nestjs": "^2.0.3",
    "@nestjs/common": "^10.1.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/typeorm": "^10.0.0",
    "@ptc-org/nestjs-query-graphql": "^3.0.2",
    "@ptc-org/nestjs-query-typeorm": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dataloader": "^2.2.2",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-yoga": "^4.0.3",
    "nestjs-seeder": "^0.3.2",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-pattern": "^5.0.4",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.4.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@parcel/watcher": "^2.2.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.71",
    "@swc/jest": "^0.2.27",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/coin(|/.*)$": "<rootDir>/libs/coin/src/$1",
      "^@app/company(|/.*)$": "<rootDir>/libs/company/src/$1",
      "^@app/entities(|/.*)$": "<rootDir>/libs/entities/src/$1",
      "^@app/dtos(|/.*)$": "<rootDir>/libs/dtos/src/$1",
      "^@app/types(|/.*)$": "<rootDir>/libs/types/src/$1",
      "^@app/social-medium(|/.*)$": "<rootDir>/libs/social-medium/src/$1",
      "^@app/sdk(|/.*)$": "<rootDir>/libs/sdk/src/$1",
      "^@app/geo(|/.*)$": "<rootDir>/libs/geo/src/$1"
    }
  }
}
